{
  "name": "tical",
  "version": "1.0.0",
  "description": "Calendar built with TypeScript and Native Web Components.",
  "main": "index.js",
  "scripts": {
    "serve": "webpack serve --progress",
    "build": "webpack --progress --env isProduction=true",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check-diagnostics": "tsc --noEmit --skipLibCheck --extendedDiagnostics",
    "type-check-all": "tsc --noEmit",
    "type-check-all-diagnostics": "tsc --noEmit --extendedDiagnostics"
  },
  "author": "Leonardo DiBella",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@types/webpack": "^5.28.0",
    "html-webpack-plugin": "^5.5.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^3.12.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.4",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
