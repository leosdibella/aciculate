{
    "name": "aciculate-api",
    "version": "1.0.0",
    "description": "Calendar backend built with TypeScript and Express",
    "main": "index.js",
    "scripts": {
        "start": "ts-node -r tsconfig-paths/register ./src/index.ts",
        "start:ssl": "ts-node -r tsconfig-paths/register ./src/index.ts --env useSsl=true",
        "type-check": "tsc --noEmit --skipLibCheck",
        "type-check-diagnostics": "tsc --noEmit --skipLibCheck --extendedDiagnostics",
        "type-check-all": "tsc --noEmit",
        "type-check-all-diagnostics": "tsc --noEmit --extendedDiagnostics"
    },
    "author": "Leonardo DiBella",
    "license": "MIT",
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-promise-router": "^3.0.0",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/node": "^18.7.14",
        "@types/pg": "^8.6.5",
        "@types/pg-format": "^1.0.2",
        "@types/reflect-metadata": "^0.1.0",
        "@types/uuid": "^8.3.4",
        "ts-node": "^10.8.1",
        "tsconfig-paths": "^3.12.0",
        "tslib": "^2.4.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "express-promise-router": "^4.1.1",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.8.0",
        "pg-format": "^1.0.4",
        "reflect-metadata": "^0.1.13",
        "socket.io": "^4.5.2",
        "uuid": "^8.3.2"
    }
}
